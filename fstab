/dev/sda8          /         ext4     defaults            1     1
/dev/sda6          /boot     ext4     defaults            1     2
/dev/sda7          /home     ext4     defaults            0     0
/dev/sda9          swap      swap     pri=1               0     0

# End /etc/fstab
